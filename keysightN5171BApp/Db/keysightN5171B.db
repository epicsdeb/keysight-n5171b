record(ai, "$(SYS)_$(SUB):$(DEV)_$(INST):FR_CSET_OUT") {
  field(DESC, "RF Output Frequency")
  field(EGU,  "MHz")
  field(PREC, "9")
  field(FLNK, "_$(SYS)_$(SUB):$(DEV)_$(INST):FR_CSET_OUT")
}

record(calcout, "_$(SYS)_$(SUB):$(DEV)_$(INST):FR_CSET_OUT") {
  field(DESC, "RF Output Frequency")
